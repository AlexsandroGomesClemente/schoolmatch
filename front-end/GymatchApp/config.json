{
  "expo": {
    "name": "GymatchApp",
    "slug": "GymatchApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "API_URL": "http://192.168.0.3:3333"
    },
    "ios": {
      "bundleIdentifier": "com.seunome.gymatch",
      "infoPlist": {
        "NSCameraUsageDescription": "O app precisa acessar a c√¢mera para tirar foto do comprovante.",
        "NSPhotoLibraryUsageDescription": "O app precisa acessar a galeria para enviar o comprovante."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.seunome.gymatch",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
